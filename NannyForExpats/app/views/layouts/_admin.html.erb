<div class="form_align">
	<h1> ADMIN PAGE </h1>

	<div class="row">
		<div class="six columns nanny_listing">
			<h3> List of Nannies </h3>
			<% Nanny.all.each do |nanny| %>
			<hr/>
			<div class="nanny_each">
				<%= nanny.email %>
				<%= image_tag nanny.avatar.url(:thumb) %>
				
			</div>
			<% end %>
		</div>

		<div class="six columns family_listing">
			<h3> List of Families </h3>
			<% Family.all.each do |family| %>
			<hr/>
			<%= link_to family_session_path do %>
			<div class="family_each">
				<%= family.email %>
			</div>

			<% end %>
			<% end %>
		</div>
	</div>

</div>